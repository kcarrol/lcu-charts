{
	"name": "@lowcodeunit/lcu-charts",
	"version": "0.0.16",
	"scripts": {
		"build:common": "ng build common",
		"build:demo": "ng build demo --prod",
		"build:lcu": "ng build lcu --prod --single-bundle && npm run pack",
		"demo": "npm run build:common && npm run start:demo",
		"demo:lcu": "npm run build:common && npm run build:lcu && npm run start:demo",
		"deploy": "npm version patch && npm run deploy:all",
		"deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo",
		"deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
		"deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
		"deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
		"ng": "ng",
		"pack": "npm run pack:lcu",
		"pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/lcu-charts.lcu.js",
		"pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
		"pack:main": "concat-glob-cli -f \"dist/lcu/main-es2015.*.js\" -o dist/lcu/wc/lcu.startup.js",
		"pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
		"start": "ng serve demo --port=4254",
		"build": "npm run build:common && npm run build:lcu",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "~8.2.5",
		"@angular/cdk": "^8.2.3",
		"@angular/common": "~8.2.5",
		"@angular/compiler": "~8.2.5",
		"@angular/core": "~8.2.5",
		"@angular/elements": "^8.2.14",
		"@angular/flex-layout": "^8.0.0-beta.27",
		"@angular/forms": "~8.2.5",
		"@angular/material": "^8.2.3",
		"@angular/platform-browser": "~8.2.5",
		"@angular/platform-browser-dynamic": "~8.2.5",
		"@angular/router": "~8.2.5",
		"@aspnet/signalr": "^1.1.4",
		"@lcu/common": "^0.79.363",
		"@lowcodeunit-devkit/ng-lcu": "^0.86.439",
		"@webcomponents/custom-elements": "^1.3.2",
		"@webcomponents/webcomponentsjs": "^2.4.1",
		"d3-array": "^2.4.0",
		"d3-brush": "^1.1.5",
		"d3-color": "^1.4.0",
		"d3-format": "^1.4.2",
		"d3-hierarchy": "^1.1.9",
		"d3-interpolate": "^1.4.0",
		"d3-scale": "^3.2.1",
		"d3-selection": "^1.4.1",
		"d3-shape": "^1.3.7",
		"d3-time-format": "^2.2.2",
		"d3-transition": "^1.3.2",
		"emoji-flags": "^1.2.0",
		"hammerjs": "^2.0.8",
		"ngx-build-plus": "^9.0.2",
		"rxjs": "~6.4.0",
		"tslib": "^1.10.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.803.22",
		"@angular-devkit/build-ng-packagr": "~0.803.22",
		"@angular/cli": "~8.3.4",
		"@angular/compiler-cli": "~8.2.5",
		"@angular/language-service": "~8.2.5",
		"@lcu/cli": "^0.24.12",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"codelyzer": "^5.0.0",
		"concat-glob-cli": "^0.1.0",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"mkdirp": "^0.5.1",
		"ng-packagr": "^5.4.0",
		"protractor": "~5.4.0",
		"rimraf": "^3.0.0",
		"ts-node": "~7.0.0",
		"tsickle": "^0.37.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3"
	}
}
